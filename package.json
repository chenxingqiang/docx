{
  "name": "docx-workspace",
  "private": true,
  "workspaces": [
    "docx-core/*",
    "docx-platforms/*"
  ],
  "scripts": {
    "web:dev": "cd docx-platforms/web && npm run dev",
    "desktop:dev": "cd docx-platforms/desktop && npm run dev",
    "mobile:ios": "cd docx-platforms/mobile/DocXMobile && npm run ios",
    "mobile:android": "cd docx-platforms/mobile/DocXMobile && npm run android",
    "cli:dev": "cd docx-platforms/cli && npm run dev"
  }
}
